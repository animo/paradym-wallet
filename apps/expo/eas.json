{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  },
  "submit": {
    "production": {
      "ios": {
        "companyName": "Animo Solutions",
        "ascAppId": "6449846111"
      },
      "android": {
        "track": "alpha"
      }
    },
    "preview": {
      "ios": {
        "companyName": "Animo Solutions",
        "ascAppId": "6449938864"
      },
      "android": {
        "track": "internal"
      }
    }
  }
}
