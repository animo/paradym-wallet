{
	"build": {
		"base": {
			"node": "22.21.1",
			"ios": {
				"image": "macos-sequoia-15.6-xcode-26.0"
			}
		},
		"development": {
			"extends": "base",
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"ios": {
				"simulator": true
			},
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"funke-production-local": {
			"extends": "base",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"EXPO_PUBLIC_APP_TYPE": "FUNKE_WALLET"
			}
		},
		"funke-preview": {
			"extends": "base",
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "preview",
				"EXPO_PUBLIC_APP_TYPE": "FUNKE_WALLET"
			}
		},
		"funke-preview-simulator": {
			"extends": "funke-preview",
			"ios": {
				"simulator": true
			}
		},
		"funke-production": {
			"extends": "base",
			"autoIncrement": true,
			"distribution": "store",
			"android": {
				"buildType": "app-bundle",
				"resourceClass": "large"
			},
			"env": {
				"EXPO_PUBLIC_APP_TYPE": "FUNKE_WALLET"
			}
		},
		"paradym-preview": {
			"extends": "base",
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "preview",
				"EXPO_PUBLIC_APP_TYPE": "PARADYM_WALLET"
			}
		},
		"paradym-preview-simulator": {
			"extends": "paradym-preview",
			"ios": {
				"simulator": true
			}
		},
		"paradym-production": {
			"extends": "base",
			"autoIncrement": true,
			"distribution": "store",
			"android": {
				"buildType": "app-bundle"
			},
			"env": {
				"EXPO_PUBLIC_APP_TYPE": "PARADYM_WALLET"
			}
		},
		"e2e-test": {
			"extends": "paradym-preview",
			"withoutCredentials": true,
			"ios": {
				"simulator": true
			},
			"android": {
				"image": "latest",
				"buildType": "apk"
			}
		}
	},
	"cli": {
		"appVersionSource": "remote"
	},
	"submit": {
		"funke-production": {
			"ios": {
				"companyName": "Animo Solutions",
				"ascAppId": "6636489314"
			},
			"android": {
				"track": "internal"
			}
		},
		"paradym-production": {
			"ios": {
				"companyName": "Animo Solutions",
				"ascAppId": "6449846111"
			},
			"android": {
				"track": "alpha"
			}
		}
	}
}
